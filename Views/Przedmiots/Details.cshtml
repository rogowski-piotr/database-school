@model Projekt_Szkola.Models.Przedmiot

@{
    ViewData["Title"] = "Szczegóły";
}

<h1>Szczegóły</h1>

<div>
    </br></br>
    <h4>Przedmiot</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nazwa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nazwa)
        </dd>
        <dt class = "col-sm-2">
            Dla Klasy
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DlaKlasy)
        </dd>
        <dt class="col-sm-2">
            Ilość godzin (tygodniowo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LiczbaGodzinTygodniowo)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.PrzedmiotID">Edytuj</a> |
    <a asp-action="Index">Powrót do listy</a>
</div>
