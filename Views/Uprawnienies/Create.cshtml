@model Projekt_Szkola.Models.Uprawnienie

@{
    ViewData["Title"] = "Dodaj uprawnienia";
}

<h1>Dodaj</h1>

<h4>Uprawnienie</h4>
</br>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                Nazwa uprawnienia
                <input asp-for="UprawnienieNazwa" class="form-control" />
                <span asp-validation-for="UprawnienieNazwa" class="text-danger"></span>
            </div>
            <div class="form-group">
                Identyfikator nauczyciela
                <select asp-for="NauczycielID" class ="form-control" asp-items="ViewBag.NauczycielID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Powrót do listy</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
